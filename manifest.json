{
    "manifest_version": 3,
    "name": "Free Dictionary",
    "description": "Double click to get the meaning",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "pic.png"
    },
    "content_scripts": [
        {
            "js": [ "main.js" ],
            "matches": [ "\u003Call_urls>" ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"]
  }